program PassOrFailSystem;

var
    ActualMark : integer;
    PossibleMark : integer;
    PercentageMark : real;
    x: integer;
begin { PassOrFailSystem }
    

    PercentageMark := (ActualMark / PossibleMark) * 100;

    if (5 >= 1) then
        
            x := (2 * (25 / 4)) + (2 * 10)
            
    { EndIf }

end. { PassOrFailSystem }